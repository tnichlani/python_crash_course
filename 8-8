def make_album(art , alb , tracks = 0):
	artist_dict = {
		'artist' : artist.title() ,
		'album' : album.title() ,
		}

print('Enter "q" anytime to stop')
	
while True:
	artist = input('Who is your favorite artist?>>> ')
	if artist == 'q':
		break
